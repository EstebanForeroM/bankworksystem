            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="typeOfProducts" layoutX="262.0" layoutY="106.0" prefHeight="25.0" prefWidth="309.0" />
      <Button fx:id="buy" layoutX="-2.0" layoutY="257.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="233.0" styleClass="buttonsTransferens" text="BUY" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id="deposit" layoutX="-6.0" layoutY="335.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="240.0" styleClass="buttonsTransferens" text="DEPOSIT" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id="withdrawals" layoutX="-6.0" layoutY="416.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="240.0" styleClass="buttonsTransferens" text="WITHDRAWALS" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id="balance" layoutX="460.0" layoutY="287.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="240.0" styleClass="buttonsTransferens2" text="BALANCE" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id="advance" layoutX="460.0" layoutY="383.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="240.0" styleClass="buttonsTransferens2" text="ADVANCE" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id="payments" layoutX="460.0" layoutY="193.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="240.0" styleClass="buttonsTransferens2" text="PAYMENTS" textFill="WHITE">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Button>
      <ImageView fitHeight="60.0" fitWidth="60.0" layoutX="490.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/logo.png" />
         </image>
      </ImageView>
      <Label layoutX="562.0" layoutY="46.0" text="Name The Bank" textFill="#fcfcfc">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
